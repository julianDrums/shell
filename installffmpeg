Invoke-WebRequest -Uri "https://github.com/BtbN/FFmpeg-Builds/releases/download/latest/ffmpeg-master-latest-win64-gpl.zip"
-OutFile "ffmpeg.zip";Expand-Archive -Path "ffmpeg.zip" -DestinationPath "C:\ffmpeg";$env:Path += ";C:\ffmpeg\bin"

Expand-Archive -Path "ffmpeg.zip" -DestinationPath "C:\ffmpeg"

